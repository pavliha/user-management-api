{
  "name": "partymaker",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "The fullstack application boilerplate for Adonisjs",
  "main": "index.js",
  "scripts": {
    "start": "adonis serve --dev",
    "pm": "pm2 start server.js",
    "migrate": "adonis migration:refresh",
    "seed": "adonis seed",
    "inspect": "nodemon --inspect server.js",
    "test": "adonis test",
    "boot": "npm run migrate && npm run seed",
    "boot:inspect": "npm run migrate && node --inspect-brk `which adonis` seed",
    "dump": "mysqldump -uroot -ppassword partymaker > database/partymaker.sql"
  },
  "keywords": [
    "partymaker"
  ],
  "author": "Partymaker team",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/ally": "^2.1.2",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.9",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/drive": "^1.0.4",
    "@adonisjs/fold": "^4.0.8",
    "@adonisjs/framework": "^5.0.12",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^5.0.4",
    "@adonisjs/mail": "^3.0.9",
    "@adonisjs/session": "^1.0.28",
    "@adonisjs/shield": "^1.0.8",
    "@adonisjs/validator": "^5.0.6",
    "@adonisjs/vow": "^1.0.17",
    "@adonisjs/websocket": "^1.0.11",
    "adonis-lucid-update-or-create": "^1.0.2",
    "auto-bind": "^1.2.1",
    "axios": "^0.19.0",
    "babel-eslint": "^8.2.3",
    "chance": "^1.0.18",
    "clean-object": "^1.0.2",
    "crypto": "^1.0.1",
    "epochta-client": "^0.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "lodash": "^4.17.11",
    "lodash-clean": "^2.0.1",
    "moment": "^2.23.0",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.4",
    "nodemon": "^1.18.9",
    "promise-serial": "^0.1.5",
    "randomstring": "latest",
    "request": "^2.88.0",
    "socket.io": "^2.2.0",
    "sqlite3": "^4.0.6",
    "uniqid": "^5.0.3",
    "url-parse": "^1.4.4",
    "util-to": "^1.0.2"
  },
  "autoload": {
    "App": "./app"
  }
}
